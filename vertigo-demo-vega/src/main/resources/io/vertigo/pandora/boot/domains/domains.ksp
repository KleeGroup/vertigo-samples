}

create Domain DO_RATING {
	dataType : Integer
	formatter : FMT_DEFAULT
	storeType : "INT"
}

create Domain DO_DATE {
	dataType : String
	formatter : FMT_DEFAULT
	storeType : "VARCHAR(20)"
	indexType : "code"
}

