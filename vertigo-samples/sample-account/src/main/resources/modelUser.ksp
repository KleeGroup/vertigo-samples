package io.vertigo.samples.account.domain

create DtDefinition DT_USER {
  id USR_ID {domain: DO_ID label: "Id" }
  field LOGIN {domain: DO_LABEL label: "Login" required:"false"}
  field NAME {domain: DO_LABEL label: "Nom" required:"false"}
  field EMAIL {domain: DO_LABEL label: "email" required:"false"}
}

create DtDefinition DT_USER_GROUP {
  id GRP_ID {domain: DO_ID label: "Id" }
  field NAME {domain: DO_LABEL label: "Nom" required:"false"}
}

create Association A_USR_COU {
  fkFieldName : "COU_ID"
    
 	dtDefinitionA : DT_USER
	dtDefinitionB : DT_COUNTRY
			
	navigabilityA : "false"
	navigabilityB : "true"
	
	multiplicityA:"0..*"
	multiplicityB:"0..1"
	
	labelA : "User"
	labelB : "Country"
	roleA : "User"
	roleB : "Country"
			
}

create Association A_USR_GRP {
  fkFieldName : "GRP_ID"
    
 	dtDefinitionA : DT_USER
	dtDefinitionB : DT_USER_GROUP
			
	navigabilityA : "false"
	navigabilityB : "true"
	
	multiplicityA:"0..*"
	multiplicityB:"0..1"
	
	labelA : "User"
	labelB : "Group"
	roleA : "User"
	roleB : "Group"
			
}